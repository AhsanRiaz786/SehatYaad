{
  "expo": {
    "name": "SehatYaad",
    "slug": "sehatyaad",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.anonymous.sehatyaad"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.sehatyaad",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "notification": {
      "color": "#007AFF",
      "androidMode": "default",
      "androidCollapsedTitle": "{{unread_count}} medication reminders"
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "color": "#007AFF",
          "sounds": [],
          "androidMode": "default",
          "androidCollapsedTitle": "Medication Reminders"
        }
      ],
      "expo-asset",
      "expo-font"
    ]
  }
}
